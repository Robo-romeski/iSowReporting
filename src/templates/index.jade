extends ./layout.jade
block content
  #page-wrapper
    .container-fluid
      // Page Heading
      .row
        .col-lg-12
          h1.page-header
            | Statistical Overview
            //- small Statistics Overview
          ol.breadcrumb
            li.active
              i.fa.fa-dashboard
              |  Dashboard
      // /.row
      .row
        .col-lg-12
          .row
            .col-lg-3.col-md-6
              .panel.panel-primary
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-user.fa-5x
                    .col-xs-9.text-right
                      for que in queseven
                        .huge #{que.avg}
                      div Average Sow Age
                a(href='#')
                  
                    .clearfix
            .col-lg-3.col-md-6
              .panel.panel-green
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-child.fa-5x
                    .col-xs-9.text-right
                      for que in quesix
                        .huge #{que.childBear}
                      div Sowers with Children
                a(href='#')
                  
                    .clearfix
            .col-lg-3.col-md-6
              .panel.panel-yellow
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-usd.fa-5x
                    .col-xs-9.text-right
                      for que in average
                        .huge #{que.amount}
                      div Average $ow Project Goal
                a(href='#')
                  
                    .clearfix
            .col-lg-3.col-md-6
              .panel.panel-red
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-users.fa-5x
                    .col-xs-9.text-right
                      for que in queone
                        .huge #{que.total}
                      div iSow Registered Users!
                a(href='#')
                  
                    .clearfix
          // /.row
          .row
            .col-lg-3.col-md-6
              .panel.panel-primary
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-usd.fa-5x
                    .col-xs-9.text-right
                      for que in quethree
                        .huge #{que.amount}
                      div Average iSow cost Goal
                a(href='#')
                  
                    .clearfix
            .col-lg-3.col-md-6
              .panel.panel-green
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-usd.fa-5x
                    .col-xs-9.text-right
                      for que in contrib
                        .huge #{que.amount}
                      div Total Contributions
                a(href='#')
                  
                    .clearfix
            .col-lg-3.col-md-6
              .panel.panel-yellow
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-shopping-cart.fa-5x
                    .col-xs-9.text-right
                      .huge 989
                      div Total Funded Projects
                a(href='#')
                  
                    .clearfix
            .col-lg-3.col-md-6
              .panel.panel-red
                .panel-heading
                  .row
                    .col-xs-3
                      i.fa.fa-child.fa-5x
                    .col-xs-9.text-right
                      .huge 904
                      div Total Children
                a(href='#')
                  
                    .clearfix
      .row
        .col-lg-12
          .panel.panel-default
            .panel-heading
              h3.panel-title
                i.fa.fa-database.fa-fw
                |  Analtyic Data
            .panel-body
              #query-output-graph
          // /.row
      .row
        .col-lg-12
          .panel.panel-default
            .panel-heading
              h3.panel-title
                i.fa.fa-bar-chart-o.fa-fw
                |  Area Chart
            .panel-body
              #morris-area-chart
              
        
       
              // <div id="morris-area-chart"></div>
              button#auth-button(hidden='') Authorize
              //textarea#query-output(cols='80', rows='20')
              //textarea#query-output-json(cols='80', rows='20')
              #query-output

        // /.row
        .row
          .col-lg-4
            .panel.panel-default
              .panel-heading
                h3.panel-title
                  i.fa.fa-long-arrow-right.fa-fw
                  |  Org Tracking
              .panel-body
                #morris-donut-charts
                .text-right
                  a(href='#')
                    //- | View Details 
                    //- i.fa.fa-arrow-circle-right
          .col-lg-4
            .panel.panel-default
              .panel-heading
                h3.panel-title
                  i.fa.fa-money.fa-fw
                  |  Top Users
              .panel-body
                .list-group
                  for results in data
                    a.list-group-item(href='#')
                      span.badge $#{results.amount}
                      i.fa.fa-fw.fa-user
                      | #{results.touser}
                .text-right
                  a(href='#')
                    //- | View All Activity 
                    //- i.fa.fa-arrow-circle-right
          .col-lg-4
            .panel.panel-default
              .panel-heading
                h3.panel-title
                  i.fa.fa-clock-o.fa-fw
                  |  Newest Users
              .panel-body
                .list-group
                  for result in total
                    a.list-group-item(href='#')
                        span.badge #{result.name} 
                        i.fa.fa-fw.fa-user
                        | #{result.id} 
                        //td #{total.name}
                .text-right
                  a(href='#')
                    //- | View All Transactions 
                    //- i.fa.fa-arrow-circle-right
          // /.row
        // /.container-fluid
      // /#page-wrapper
    // /#wrapper
    // jQuery
    script(src='js/jquery.js')
    // Angular
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.min.js')
    // Bootstrap Core JavaScript
    script(src='js/bootstrap.min.js')
    // Morris Charts JavaScript
    script(src='js/plugins/morris/raphael.min.js')
    script(src='js/plugins/morris/morris.min.js')
    script(src='js/plugins/morris/morris-data.js')
    script(src='js/google.js')
    script(src='js/ang.dbquery.js')
    script(src='https://apis.google.com/js/client.js?onload=authorize')

